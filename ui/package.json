{
  "name": "ozone-studio-ui",
  "version": "0.3.0",
  "description": "Ozone Studio - Omnidirectional Zero-Shot Neural Engine UI",
  "author": {
    "name": "Christian Liz-Fonts",
    "email": "admin@ozonestudio.xyz"
  },
  "homepage": "https://github.com/RebornBeat/Ozone-Studio/",
  "repository": {
    "type": "git",
    "url": "https://github.com/RebornBeat/Ozone-Studio.git"
  },
  "license": "MIT",
  "main": "electron/main.js",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vite build && electron-builder"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.0",
    "electron": "^27.0.0",
    "electron-builder": "^24.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "wait-on": "^7.0.0"
  },
  "build": {
    "appId": "com.ozone.studio",
    "productName": "Ozone Studio",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "mac": {
      "target": ["dmg", "zip"],
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": ["nsis", "portable"]
    },
    "linux": {
      "target": ["AppImage", "deb"],
      "category": "Development",
      "maintainer": "Christian Liz-Fonts <admin@ozonestudio.xyz>"
    }
  }
}
